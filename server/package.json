{
  "name": "aqua-manage-fish-server",
  "version": "1.0.0",
  "description": "Fish Selling Management System Server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "tsx src/index.ts",
    "lint": "eslint src/**/*.ts",
    "migrate:test": "node ../scripts/migrate-to-supabase.js test",
    "migrate:schema": "node ../scripts/migrate-to-supabase.js schema",
