    "migrate:import": "node ../scripts/migrate-to-supabase.js import",
    "migrate:full": "node ../scripts/migrate-to-supabase.js migrate",
    "test:local": "node ../scripts/test-local-db.js",
    "test:supabase": "node ../scripts/test-supabase.js",
    "test:supabase-client": "node ../scripts/test-supabase-client.js",
    "test:connection": "node ../scripts/test-backend-connection.js",
    "test:cloudinary": "tsx utils/cloudinaryTest.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "node dist/scripts/migrate.js",
    "db:seed": "node dist/scripts/seed.js",
    "manage-users": "tsx scripts/manage-existing-users.ts"
  },
  "keywords": [
    "fish",
    "management",
    "typescript",
    "express",
    "postgresql"
